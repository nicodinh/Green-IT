			protected function button_clickHandler_15():void
			{
				Sequence30.play();
				if (currentState == 'coursDematerialisation1')
					currentState = 'coursDematerialisation2';
				else if (currentState == 'coursDematerialisation2')
					currentState = 'coursDematerialisation3';
				
				if (currentState == 'coursRecyclage1')
					currentState = 'coursRecyclage2';
				
				if (currentState == 'coursVirtualisation1')
					currentState = 'coursVirtualisation2';
				else if (currentState == 'coursVirtualisation2')
					currentState = 'coursVirtualisation3';
				
				if (currentState == 'coursTeletravail1')
					currentState = 'coursTeletravail2';
				
				if (currentState == 'coursSoftware1')
					currentState = 'coursSoftware2';
				else if (currentState == 'coursSoftware2')
					currentState = 'coursSoftware3';
				
				if (currentState == 'coursSensibilisation1')
					currentState = 'coursSensibilisation2';
				
				if (currentState == 'coursRenouvellement1')
					currentState = 'coursRenouvellement2';
				
				if (currentState == 'coursPlan1')
					currentState = 'coursPlan2';
				else if (currentState == 'coursPlan2')
					currentState = 'coursPlan3';
				else if (currentState == 'coursPlan3')
					currentState = 'coursPlan4';
	
			}
	
	
			protected function button_clickHandler_16():void
