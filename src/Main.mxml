				if (currentState == 'coursVirtualisation3')
					currentState = 'coursVirtualisation2';
				else if (currentState == 'coursVirtualisation2')
					currentState = 'coursVirtualisation1';
				
				if (currentState == 'coursTeletravail2')
					currentState = 'coursTeletravail1';
				
				if (currentState == 'coursSoftware3')
					currentState = 'coursSoftware2';
				else if (currentState == 'coursSoftware2')
					currentState = 'coursSoftware1';
				
				if (currentState == 'coursSensibilisation2')
					currentState = 'coursSensibilisation1';
				
				if (currentState == 'coursRenouvellement2')
					currentState = 'coursRenouvellement1';
				
				if (currentState == 'coursPlan4')
					currentState = 'coursPlan3';
				else if (currentState == 'coursPlan3')
					currentState = 'coursPlan2';
				else if (currentState == 'coursPlan2')
					currentState = 'coursPlan1';
	
			}
	
